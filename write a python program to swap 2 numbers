import sys

num1 = input("Enter the first number:")
num2 = input("Enter the second number:")

print("The first number is" +" " + num1 + "the second number is" + " " +num2)

print("Swapping the numbers")

num1 = int(num1) + int(num2)
num2 = int(num1) - int(num2)
num1 = int(num1) - int(num2)

print("After swapping the numbers " +" "+ "num1 :" + str(num1) +"num2 :" +str(num2))
